{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "app:start": "expo start",
    "app:android": "expo start --android",
    "app:ios": "expo start --ios",
    "app:web": "expo start --web",
    "app:eject": "expo eject",
    "app:watch:worker": "chokidar './src/worker.ts' './src/WebWorker/bootstrap.ts' -c 'yarn app:build:worker'",
    "app:build:worker": "yarn app:build:worker:app && yarn app:build:worker:bootstrap",
    "app:build:worker:app": "esbuild  ./src/worker.ts --bundle | node ./bin/stringify.js > ./src/worker.inlined.ts",
    "app:build:worker:bootstrap": "esbuild  ./src/WebWorker/bootstrap.ts --bundle | node ./bin/stringify.js > ./src/WebWorker/bootstrap.inlined.ts"
  },
  "dependencies": {
    "@s-libs/micro-dash": "^11.2.1",
    "@types/jest": "^26.0.20",
    "@types/prompts": "^2.0.9",
    "@types/redis": "^2.8.28",
    "@types/ws": "^7.4.0",
    "expo": "~40.0.0",
    "expo-asset": "~8.2.1",
    "expo-status-bar": "~1.0.3",
    "firebase-admin": "^9.5.0",
    "firebase-functions": "^3.13.1",
    "georedis": "^3.1.3",
    "gpxparser": "^3.0.7",
    "isomorphic-ws": "^4.0.1",
    "nanoevents": "^5.1.11",
    "prompts": "^2.4.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-40.0.1.tar.gz",
    "react-native-maps": "0.27.1",
    "react-native-web": "~0.13.12",
    "react-native-webview": "11.0.0",
    "redis": "^3.0.2",
    "smart-buffer": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@types/react": "~16.9.35",
    "@types/react-dom": "~16.9.8",
    "@types/react-native": "~0.63.2",
    "chokidar-cli": "^2.1.0",
    "esbuild": "^0.8.48",
    "prettier": "^2.2.1",
    "prettier-plugin-organize-imports": "^1.1.1",
    "typescript": "~4.0.0"
  },
  "private": true
}
