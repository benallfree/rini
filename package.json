    "esbuild": "^0.8.48",
    "eslint": "^7.20.0",
    "eslint-config-universe": "^7.0.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.10.0",
    "prettier": "^2.2.1",
