{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "app:start:prod": "expo start --no-dev --minify",
    "app:start": "expo start",
    "app:android": "expo start --android",
    "app:ios": "expo start --ios",
    "app:web": "expo start --web",
    "app:eject": "expo eject",
    "app:watch:worker": "chokidar './src/app/components/Worker/worker-app.ts' './src/rn-webworker/bootstrap.ts' -c 'yarn app:build:worker'",
    "app:build:worker": "yarn app:build:worker:app && yarn app:build:worker:bootstrap",
    "esbuild": "esbuild --bundle --minify --sourcemap=inline --tsconfig=./tsconfig.json",
    "app:build:worker:app": "yarn run --silent esbuild ./src/app/components/Worker/worker-app.ts  | node ./bin/stringify.js 1> ./src/app/components/Worker/worker-app.inlined.ts",
    "app:build:worker:bootstrap": "yarn run --silent esbuild ./src/rn-webworker/bootstrap.ts | node ./bin/stringify.js 1> ./src/rn-webworker/bootstrap.inlined.ts",
    "bot:dev": "ts-node-dev --respawn --transpile-only src/bot/run.ts",
    "serve:dev": "ts-node-dev --respawn --transpile-only src/server/index.ts"
  },
  "dependencies": {
    "@expo/metro-config": "^0.1.56",
    "@reduxjs/toolkit": "^1.5.0",
    "@s-libs/micro-dash": "^11.2.1",
    "@types/jest": "^26.0.20",
    "@types/latlon-geohash": "^2.0.0",
    "@types/prompts": "^2.0.9",
    "@types/react-redux": "^7.1.16",
    "@types/redis": "^2.8.28",
    "@types/serialize-error": "^4.0.1",
    "@types/sharp": "^0.27.1",
    "@types/ws": "^7.4.0",
    "avatar-builder": "^1.0.1",
    "bottleneck": "^2.19.5",
    "expo": "~40.0.0",
    "expo-asset": "~8.2.1",
    "expo-av": "~8.7.0",
    "expo-device": "~2.4.0",
    "expo-file-system": "~9.3.0",
    "expo-firebase-recaptcha": "^1.3.0",
    "expo-location": "~10.0.0",
    "expo-permissions": "~10.0.0",
    "expo-status-bar": "~1.0.3",
    "expo-task-manager": "~8.6.0",
    "firebase": "7.9.0",
    "firebase-admin": "^9.5.0",
    "firebase-functions": "^3.13.1",
    "geolib": "^3.3.1",
    "georedis": "^3.1.3",
    "gpxparser": "^3.0.7",
    "install": "^0.13.0",
