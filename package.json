{
  "name": "ayo",
  "version": "0.0.1",
  "description": "Say hi to others",
  "main": "dist/index.js",
  "author": "Ben Allfree",
  "license": "MIT",
  "scripts": {
    "clean": "concurrently --raw 'pnpm:clean:*'",
    "clean:common": "pnpm common clean",
    "clean:bot": "pnpm bot clean",
    "clean:server": "pnpm run server clean",
    "clean:client": "pnpm client clean",
    "clean:georedis-promised": "pnpm georedis clean",
    "build": "pnpm common build && pnpm client build && pnpm bot build && pnpm georedis build && pnpm run server build ",
    "watch": "pnpm clean && pnpm build && concurrently --raw 'pnpm:watch:*'",
    "watch:common": "pnpm common watch",
    "watch:bot": "pnpm bot watch",
    "watch:server": "pnpm run server watch",
    "watch:client": "pnpm client watch",
    "watch:georedis-promised": "pnpm georedis watch",
    "georedis": "cd packages/georedis-promised && pnpm",
    "common": "cd packages/common && pnpm",
    "admin": "cd packages/admin && pnpm",
    "bot": "cd packages/bot && pnpm",
    "client": "cd packages/client && pnpm",
    "server": "cd packages/server && pnpm"
  },
  "private": true,
  "devDependencies": {
    "concurrently": "^5.3.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.10.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "prettier-plugin-organize-imports": "^1.1.1",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.19",
    "typescript": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.18.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "plugins": [
      "./node_modules/prettier-plugin-organize-imports"
    ]
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  }
}
