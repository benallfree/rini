{
  "expo": {
    "entryPoint": "./src/app/App.tsx",

    "name": "Rini",
    "slug": "rini",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/app/assets/generated/icon.png",
    "splash": {
      "image": "./src/app/assets/generated/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 30000
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.benallfree.rini",
      "buildNumber": "3",
      "infoPlist": {
        "UIBackgroundModes": ["location", "fetch"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/app/assets/generated/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./src/app/assets/generated/favicon.png"
    }
  }
}
