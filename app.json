{
  "expo": {
    "entryPoint": "./src/app/App.tsx",

    "name": "app",
    "slug": "app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/app/assets/icon.png",
    "splash": {
      "image": "./src/app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.benallfree.rini"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./src/app/assets/favicon.png"
    }
  }
}
