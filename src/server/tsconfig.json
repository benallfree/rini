{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "moduleResolution": "node",
    "baseUrl": ".",
    "outDir": "../../build/server",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "typeRoots": ["../@types", "../../node_modules/@types"]
  },
  "include": ["**/*"]
}
