{
  "name": "@rini/app",
  "version": "0.0.1",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios --no-packager",
    "build": "eas build --platform ios",
    "submit": "EXPO_APPLE_APP_SPECIFIC_PASSWORD=`cat ../../.secrets/apple-app-specific-password` eas submit -p ios",
    "eject:clean": "rm -rf ios",
    "eject": "expo eject -p ios",
    "reset:ios": "yarn eject:clean && yarn eject",
    "pod": "cd ios && pod install && cd .."
  },
  "dependencies": {
    "@babel/core": "~7.9.0",
    "@react-native-community/cli-platform-ios": "^4.13.0",
    "@react-native-firebase/app": "^10.5.0",
    "@react-native-firebase/auth": "^10.5.1",
    "@rini/common": "../common",
    "@types/react": "~16.9.35",
    "@types/react-dom": "~16.9.8",
    "@types/react-native": "~0.63.2",
    "babel-jest": "~25.2.6",
    "buffer": "^6.0.3",
    "expo": "~40.0.0",
    "expo-cli": "^4.0.17",
    "expo-splash-screen": "~0.8.0",
    "expo-status-bar": "~1.0.3",
    "expo-updates": "~0.4.0",
    "get-yarn-workspaces": "^1.0.2",
    "jest": "~25.2.6",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "~0.63.4",
    "react-native-elements": "^3.1.0",
    "react-native-gesture-handler": "~1.8.0",
    "react-native-phone-number-input": "^2.0.0",
    "react-native-reanimated": "~1.13.0",
    "react-native-screens": "~2.15.0",
    "react-native-udp": "^4.1.2",
    "react-native-unimodules": "~0.12.0",
    "react-native-vector-icons": "^8.0.0",
    "react-native-web": "~0.13.12",
    "react-test-renderer": "~16.13.1",
    "typescript": "^4.1.3",
    "unstated-next": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "babel-jest": "~25.2.6",
    "jest": "~25.2.6",
    "react-test-renderer": "~16.13.1"
  },
  "private": true
}
